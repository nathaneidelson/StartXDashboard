{
  "manifest_version": 2,

  "name": "StartX_Dashboard",
  "description": "This extension serves as a link dashboard for StartX employees.",
  "version": "1.0",

    "permissions": [
        "https://mail.google.com/",
        "https://drive.google.com/",
        "http://startx.stanford.edu/"
    ],
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
